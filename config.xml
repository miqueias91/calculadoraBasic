<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.calculadorabasic" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Quiz da Bíblia</name>
    <description>
        Teste seus conhecimentos bíblicos através deste divertido e educativo jogo de perguntas e respostas bíblicas.
    </description>
    <author email="miqueiasmcaetano@gmail.com" href="https://miqueiasmcaetano.000webhostapp.com">
        Innovate Soft
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash src="./www/img/Default@2x~universal~anyany.png" />
    <splash src="./www/img/Default@2x~universal~comany.png" />
    <splash src="./www/img/Default@2x~universal~comcom.png" />
    <splash src="./www/img/Default@3x~universal~anyany.png" />
    <splash src="./www/img/Default@3x~universal~anycom.png" />
    <splash src="./www/img/Default@3x~universal~comany.png" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="./www/img/logo.png" />
        <icon density="mdpi" src="./www/img/logo.png" />
        <icon density="hdpi" src="./www/img/logo.png" />
        <icon density="xhdpi" src="./www/img/logo.png" />
    </platform>
    <preference name="phonegap-version" value="cli-8.0.0" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="Orientation" value="portrait" />
    <preference name="windows-appx-target" value="8.1-phone" />
    <preference name="WindowsToastCapable" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <plugin name="kr.co.joycorp.cordova.exitapp" spec="~1.0.0" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="onesignal-cordova-plugin" spec="^2.4.2" />
    <plugin name="cordova-plugin-exitapp" spec="~1.0.0" />
    <engine name="android" spec="^7.0.0" />
</widget>
